{
  "name": "base-typescript-express",
  "version": "0.1.0",
  "description": "Starter template for Typescript and Express projects",
  "author": "Luiz Pereira",
  "scripts": {
    "start": "ts-node src/app.ts",
    "lint": "eslint 'src/*.ts'",
    "lint:fix": "eslint 'src/*.ts' --fix",
    "test:unit": "_mocha -r ts-node/register 'src/**/*Spec.ts'",
    "test:interface": "_mocha -r ts-node/register 'src/endpoints/test/**/*Test.ts'"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.0.4",
    "@types/superagent": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "eslint": "^7.14.0",
    "express": "^4.17.1",
    "superagent": "^6.1.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=12.16.0"
  }
}
